{
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "containerUser": "alnvdl",
  "onCreateCommand": "CONFIG=alnvdl ./create.sh",
  "postStartCommand": "./start.sh",
  "containerEnv": {
    "SHELL": "/bin/bash",
    "DEBIAN_FRONTEND": "noninteractive",
    "PATH": "${localEnv:PATH}:/home/alnvdl/dev/go/bin:/home/alnvdl/dev/node/bin"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "golang.go",
        "ms-python.flake8",
        "ms-python.mypy-type-checker",
        "ms-python.black-formatter",
        "dbaeumer.vscode-eslint",
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ]
    },
    "codespaces": {
      "repositories": {
        "alnvdl/terr": {
          "permissions": "write-all"
        },
        "alnvdl/fstringen": {
          "permissions": "write-all"
        },
        "alnvdl/request": {
          "permissions": "write-all"
        },
        "alnvdl/alnvdl.github.io": {
          "permissions": "write-all"
        },
        "alnvdl/squarewars": {
          "permissions": "write-all"
        },
        "alnvdl/squareplanner": {
          "permissions": "write-all"
        },
        "alnvdl/oks": {
          "permissions": "write-all"
        },
        "alnvdl/angst": {
          "permissions": "write-all"
        },
        "alnvdl/vscode": {
          "permissions": "write-all"
        },
        "alnvdl/varys": {
          "permissions": "write-all"
        },
        "alnvdl/diagrama": {
          "permissions": "write-all"
        },
        "alnvdl/lightwebapp": {
          "permissions": "write-all"
        },
        "alnvdl/apurator": {
          "permissions": "write-all"
        },
        "alnvdl/anything": {
          "permissions": "write-all"
        }
      }
    }
  }
}
